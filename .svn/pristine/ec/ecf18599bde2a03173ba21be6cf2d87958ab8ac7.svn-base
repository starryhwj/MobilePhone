            title: '备注',
            sortable: false,
        },{
            field: 'Actions',
            title: '操作',
            sortable: false,
            width: 110,
            overflow: 'visible',
	        autoHide: false,
	        textAlign: 'center',
            template: function(row) {
	            return '\
	                <button class="btn btn-sm btn-clean btn-icon btn-icon-sm" title="新建任务" data-mission-id="' + row.id + '">\
